{
  "name": "twemoji-explorer",
  "version": "0.0.1",
  "description": "Website for exploring emojis.",
  "private": "true",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rm -rf ./public",
    "build": "npm run clean && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "NODE_ENV=development rollup  -c -w --no-watch.clearScreen",
    "serve": "npm run clean && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.9",
    "@elderjs/elderjs": "^1.6.13",
    "@elderjs/plugin-browser-reload": "^1.3.9",
    "@elderjs/plugin-images": "^1.3.9",
    "autoprefixer": "^10.4.4",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "emojilib": "^3.0.6",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "flexsearch": "^0.6.32",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.1",
    "intersection-observer": "^0.12.0",
    "nodemon": "^2.0.20",
    "polka": "^0.5.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.70.2",
    "sirv": "^2.0.2",
    "svelte": "3.47.0",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "twemoji-parser": "^14.0.0",
    "typescript": "^4.6.3",
    "unicode-emoji-json": "^0.3.1",
    "vanilla-lazyload": "^17.7.0"
  }
}
